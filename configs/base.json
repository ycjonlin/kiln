{
  "dataset": {
    "type": "event",
    "name": "ml-1m",
    "path": "datasets",
    "min_user": 20,
    "min_item": 20,
    "split_ratio":0.8,
    "train": {
      "batch_size": 20,
      "split_ratio": 0.8,
      "shuffle": true
    },
    "test": {
      "batch_size": 20,
      "split_ratio": 0.8,
      "shuffle": false
    }
  },
  "model": {
    "name": "ae",
    "embedding_dim": 50,
    "inner": {
      "name": "mlp",
      "num_layers": 3,
      "hidden_dim": 200,
      "dropout": {
        "name": "bernoulli",
        "std": 0,
        "layers": []
      }
    }
  },
  "fit": {
    "epochs": 50,
    "samples": 1,
    "loss_fn": {
      "name": "multi-ce"
    },
    "optimizer": {
      "name": "sgd",
      "momentum": 0.9,
      "learning_rate": 1e-3,
      "weight_decay": 0
    }
  },
  "metrics": {
    "ranking": {
      "topks": [50],
      "weights": ["recall", "ndcg", "mrr"],
      "is_dist": false,
      "is_multi": true,
      "eps": 1e-10
    }
  }
}